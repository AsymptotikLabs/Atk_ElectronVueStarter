    "electron-reloader": "^1.2.1",
    "eslint": "^7.26.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.0.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4",
    "vue-jest": "^5.0.0-0"
